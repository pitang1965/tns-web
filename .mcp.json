{
  "mcpServers": {
    "mongodb-readonly": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@mongodb-js/mongodb-mcp-server"
      ],
      "env": {
        "MDB_MCP_CONNECTION_STRING": "${MONGODB_URI_READONLY}",
        "MDB_MCP_READ_ONLY": "true"
      }
    }
  }
}